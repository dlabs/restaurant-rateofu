        "module": "commonjs",
        "sourceMap": true,
        "outDir": "./dist",
        "rootDir": "./",
        "noEmitOnError": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
